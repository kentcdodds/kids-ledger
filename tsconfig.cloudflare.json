{
  "extends": "./tsconfig.json",
  "include": [
    ".react-router/types/**/*",
    "app/**/*",
    "app/**/.server/**/*",
    "app/**/.client/**/*",
    "worker/**/*",
    "types/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "strict": true,
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "types": ["vite/client"],
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "rootDirs": [".", "./.react-router/types"],
    "paths": {
			"#app/*": ["./app/*"],
			"#tests/*": ["./tests/*"],
			"#types/*": ["./types/*"],
			"#worker/*": ["./worker/*"]
		},
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
